{
  "name": "athena",
  "version": "0.3.0",
  "private": false,
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "mocha --opts ./test/.mocha.opts ./test/**/*Spec.js",
    "lint": "vue-cli-service lint",
    "api:lint": "eslint --cache --no-eslintrc -c ./server/.eslintrc.js server/*.js server/**/*.js test/**/*.js bin/*.js",
    "api:fix": "eslint --fix --no-eslintrc -c ./server/.eslintrc.js server/*.js server/**/*.js test/**/*.js bin/*.js",
    "api:serve": "./bin/cli.js --watch --lint",
    "docker:build": "docker build -t athena ."
  },
  "bin": {
    "athena": "./bin/cli.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "@mdi/font": "^3.5.95",
    "barrkeep": "^1.3.3",
    "cron": "^1.7.0",
    "dockerode": "^2.5.8",
    "find-free-port": "^2.0.0",
    "minimist": "^1.2.0",
    "moment": "^2.24.0",
    "mongodb": "^3.2.2",
    "net-ping": "^1.2.3",
    "request": "^2.88.0",
    "requireindex": "^1.2.0",
    "restify": "^8.2.0",
    "uuid": "^3.3.2",
    "vue": "^2.6.10",
    "vue-native-websocket": "^2.0.12",
    "vue-router": "^3.0.1",
    "vue-sparklines": "^0.1.9",
    "vuetify": "^1.5.7",
    "watershed": "^0.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.1",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-service": "^3.5.1",
    "@vue/eslint-config-standard": "^4.0.0",
    "glob-watcher": "^5.0.3",
    "mocha": "^6.0.2",
    "should": "^13.2.3",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-template-compiler": "^2.6.10"
  }
}
